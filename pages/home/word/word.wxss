page {
  background-color: #f8f8f8;
  height: 100%;
  position: relative;
  background-origin: content-box;
  background-image: url('data:image/png;base64,');
  background-position-x: 225rpx;
  background-position-y: 80rpx;
  width: 100%;
  /* z-index: 1000; */
  /* height: 480rpx; */
  background-repeat: no-repeat;
  background-size: 100% 40%;
  -moz-background-size: 100% 40%;
}

.container {
  width: 100%;
}

.background-img {
  /* position: absolute; */
  background-position-x: 225rpx;
  background-position-y: 80rpx;
  width: 100%;
  z-index: 1000;
  height: 480rpx;
  position: absolute;
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
}

.background-img image {
  height: 480rpx;
  width: 100%;
  margin-left: 30%;
  margin-top: 80rpx;
}

.header-container {
  display: flex;
  flex-direction: row;
  position: relative;
  margin-right: 25rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  margin-left: 25rpx;
}

.header-container-left {
  font-size: 28rpx;
  color: #b8b8b8;
}

.header-container-right {
  /* text-decoration: underline; */
  width: 700rpx;
  font-size: 28rpx;
  margin-top: 5rpx;
  /* text-align: end; */
  margin-right: 25rpx;
  margin-left: 25rpx;
  /* right: 0; */
  color: #888;
}

.progress-container {
  width: 710rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 15rpx;
  height: 10rpx;
  background-color: white;
  border-radius: 4px;
  border-width: 1px;
  border-color: #2f2f2f;
}

.progress {
  background-color: #c3f4ec;
  border-radius: 4px;
  height: 10rpx;
}

.word-sentence {
  margin: 20rpx 25rpx 20rpx 25rpx;
}

.word-sentence text {
  font-size: 36rpx;
}

.word-sentence rich-text {
  font-size: 36rpx;
  z-index: 9999;
}

.word-symbol-container {
  display: flex;
  flex-direction: column;
  margin-top: 25rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
}

.word {
  font-size: 30rpx;
  font-weight: bolder;
}

.symbol {
  font-size: 25rpx;
  color: #c8c8c8;
}

.word-meaning-container {
  margin-top: 20rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
}

.word-meaning-container text {
  font-size: 30rpx;
  color: #b8b8b8;
}

.choose-container {
  margin: 45rpx 25rpx 0 25rpx;
}

.graph-container {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.judge-graph-container {
  width: 342rpx;
  margin-left: 4rpx;
  margin-right: 4rpx;
  margin-top: 4rpx;
  margin-bottom: 4rpx;
  height: 248rpx;
  position: relative;
}

.judge-graph-container-image {
  width: 342rpx;
  height: 248rpx;
  border-radius: 10rpx;
}

.judge {
  width: 180rpx;
  height: 160rpx;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -90rpx;
  margin-top: -80rpx;
}

.text-container {
  display: flex;
  flex-direction: column;
}

.judge-text-container {
  padding-top: 30rpx;
  padding-bottom: 30rpx;
  padding-left: 20rpx;
  padding-right: 20rpx;
  margin-top: 2rpx;
  margin-bottom: 2rpx;
  background-color: #fff;
  position: relative;
}

.judge-text-container text {
  font-size: 28rpx;
  padding-left: 30rpx;
  padding-right: 10rpx;
  color: #3a3a3a;
}

.judge-text-container image {
  position: absolute;
  right: 20rpx;
  width: 60rpx;
  height: 60rpx;
  top: 50%;
  margin-top: -30rpx;
}

.sound-container {
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  margin-left: -64.5rpx;
  width: 129rpx;
  height: 129rpx;
}

.sound-container image {
  width: 100%;
  height: 100%;
}

.toast {
  width: 280rpx;
  font-size: 26rpx;
  text-align: center;
  position: fixed;
  bottom: 180rpx;
  left: 50%;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  margin-left: -140rpx;
  color: white;
  background-color: #adadadcc;
  border-radius: 5px;
}
